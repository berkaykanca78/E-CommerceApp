{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ECommerce;Integrated Security=True;Encrypt=False;TrustServerCertificate=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "http://localhost:3000",
      "https://localhost:4200"
    ]
  },
  "Development": {
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  }
}
